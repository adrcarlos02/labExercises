    "/api/calculator/add": {
      "get": {
        "summary": "Add two numbers",
        "description": "Returns the sum of two numbers",
        "parameters": [
          {
            "name": "num1",
            "in": "query",
            "description": "First number",
            "required": true,
            "type": "number"
          },
          {
            "name": "num2",
            "in": "query",
            "description": "Second number",
            "required": true,
            "type": "number"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation",
            "schema": {
              "type": "object",
              "properties": {
                "result": {
                  "type": "number"
                }
              }
            }
          },
          "400": {
            "description": "Invalid input"
          }
        }
      }
    },
